--
-- Name: protect_draft_fields(); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION public.protect_draft_fields() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
BEGIN
  IF OLD.status NOT IN ('draft','rejected') THEN
    IF (NEW.pad_id     IS DISTINCT FROM OLD.pad_id)
    OR (NEW.reference  IS DISTINCT FROM OLD.reference)
    OR (NEW.content    IS DISTINCT FROM OLD.content) THEN
      RAISE EXCEPTION 'Documento en %, no se puede editar contenido/metadata', OLD.status;
    END IF;
  END IF;
  NEW.last_modified_at := now();
  RETURN NEW;
END $$;



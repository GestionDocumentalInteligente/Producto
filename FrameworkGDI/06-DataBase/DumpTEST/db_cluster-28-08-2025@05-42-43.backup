--
-- Name: assert_signers_editable(); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION public.assert_signers_editable() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
DECLARE v_status document_status;
BEGIN
  SELECT status INTO v_status
  FROM document_draft
  WHERE document_id = COALESCE(NEW.document_id, OLD.document_id);

  IF v_status IS NULL THEN
    RAISE EXCEPTION 'Documento % no existe', COALESCE(NEW.document_id, OLD.document_id);
  END IF;

  IF v_status NOT IN ('draft','rejected') THEN
    RAISE EXCEPTION 'No se pueden modificar firmantes cuando el documento está en %', v_status;
  END IF;

  RETURN CASE WHEN TG_OP = 'DELETE' THEN OLD ELSE NEW END;
END $$;



-- Name: tg_call_documents_send_to_sign(); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION public.tg_call_documents_send_to_sign() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
DECLARE v_actor uuid;
BEGIN
  IF NOT (NEW.status = 'sent_to_sign' AND OLD.status IN ('draft','rejected')) THEN
    RETURN NEW;
  END IF;

  -- actor (Supabase) o fallback a JWT
  BEGIN
    v_actor := auth.uid();
  EXCEPTION WHEN OTHERS THEN
    v_actor := NULLIF(current_setting('request.jwt.claim.sub', true), '')::uuid;
  END;

  PERFORM documents_send_to_sign(OLD.document_id, v_actor);

  -- Cancelamos el UPDATE original (ya actualizó la función)
  RETURN NULL;
END $$;



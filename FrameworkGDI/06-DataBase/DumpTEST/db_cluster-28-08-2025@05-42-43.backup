-- Name: move_head_user_to_priv_sector(); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION public.move_head_user_to_priv_sector() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
DECLARE
  priv_sector_id UUID;
BEGIN
  -- Solo actuamos si el head_user_id cambió
  IF NEW.head_user_id IS DISTINCT FROM OLD.head_user_id AND NEW.head_user_id IS NOT NULL THEN
    
    -- Buscamos el sector PRIV correspondiente al department
    SELECT sector_id INTO priv_sector_id
    FROM sectors
    WHERE department_id = NEW.department_id
      AND acronym = 'PRIV';

    -- Aseguramos que existe ese sector PRIV
    IF priv_sector_id IS NULL THEN
      RAISE EXCEPTION 'No se encontró el sector PRIV para este department.';
    END IF;

    -- Actualizamos al usuario para que esté en ese sector
    UPDATE users
    SET sector_id = priv_sector_id
    WHERE user_id = NEW.head_user_id;
  END IF;

  RETURN NEW;
END;
$$;



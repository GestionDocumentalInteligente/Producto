--

CREATE FUNCTION public.assign_acronym_from_municipality() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
DECLARE
  muni_acronym TEXT;
BEGIN
  -- Solo si es raíz y no tiene acrónimo
  IF NEW.parent_jurisdiction_id IS NULL AND NEW.acronym IS NULL THEN
    -- Buscamos el acrónimo de la ciudad asociada
    SELECT acronym INTO muni_acronym
    FROM municipalities
    WHERE id_municipality = NEW.municipality_id;

    -- Si lo encuentra, lo asigna
    IF muni_acronym IS NOT NULL THEN
      NEW.acronym := muni_acronym;
    ELSE
      RAISE EXCEPTION 'No se pudo encontrar el acrónimo de la municipalidad asociada.';
    END IF;
  END IF;

  RETURN NEW;
END;
$$;



  -- Si el status cambió a 'sent_to_sign' y antes era otro
  IF NEW.status = 'sent_to_sign' AND OLD.status IS DISTINCT FROM NEW.status THEN
    -- Solo actualiza si aún no tiene fecha seteada
    IF NEW.sent_to_sign_at IS NULL THEN
      NEW.sent_to_sign_at := NOW();
    END IF;
  END IF;

  RETURN NEW;
END;
$$;



--
-- Name: validate_document_signers(); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION public.validate_document_signers() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
BEGIN
  -- (A) No duplicar firmante en el mismo documento
  IF EXISTS (
    SELECT 1
    FROM document_signers ds
    WHERE ds.document_id = NEW.document_id
      AND ds.user_id = NEW.user_id
      AND ds.document_signer_id <> COALESCE(NEW.document_signer_id, '00000000-0000-0000-0000-000000000000'::uuid)
  ) THEN
    RAISE EXCEPTION 'El usuario ya está asignado como firmante en este documento.';
  END IF;

  -- (B) Reglas de signing_order según sea numerador o no
  IF NEW.is_numerator IS TRUE THEN
    -- Solo puede haber un numerador por documento
    IF EXISTS (
      SELECT 1
      FROM document_signers ds
      WHERE ds.document_id = NEW.document_id
        AND ds.is_numerator = TRUE
        AND ds.document_signer_id <> COALESCE(NEW.document_signer_id, '00000000-0000-0000-0000-000000000000'::uuid)
    ) THEN
      RAISE EXCEPTION 'Solo puede haber un numerador por documento.';
    END IF;

    -- El numerador debe tener signing_order (si viene NULL, lo seteo a 1)
    IF NEW.signing_order IS NULL THEN
      NEW.signing_order := 1;
    END IF;

  ELSE
    -- Firmantes comunes: NO deben tener signing_order
    IF NEW.signing_order IS NOT NULL THEN
      RAISE EXCEPTION 'Solo el numerador puede tener signing_order; los firmantes comunes firman en paralelo.';
    END IF;
  END IF;

  RETURN NEW;
END;
$$;


ALTER FUNCTION public.validate_document_signers() OWNER TO postgres;


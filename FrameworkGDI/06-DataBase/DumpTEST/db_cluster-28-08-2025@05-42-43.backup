CREATE FUNCTION public.set_end_date_on_deactivation() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
BEGIN
  -- Si se desactiva y no hay end_date, lo seteamos
  IF NEW.is_active = false AND OLD.is_active = true THEN
    IF NEW.end_date IS NULL THEN
      NEW.end_date := current_date;
    END IF;
  END IF;

  -- Si se reactiva, borramos el end_date (opcional)
  IF NEW.is_active = true AND OLD.is_active = false THEN
    NEW.end_date := NULL;
  END IF;

  RETURN NEW;
END;
$$;



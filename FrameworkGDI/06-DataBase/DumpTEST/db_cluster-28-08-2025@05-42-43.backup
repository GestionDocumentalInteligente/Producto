--
-- Name: create_document_version(); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION public.create_document_version() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
DECLARE
  next_version integer;
BEGIN
  -- Solo si el estado anterior es 'draft' y el nuevo no lo es
  IF OLD.status = 'draft' AND NEW.status != 'draft' THEN
    -- Calcular la próxima versión
    SELECT COALESCE(MAX(version_number), 0) + 1
    INTO next_version
    FROM document_versions
    WHERE document_id = NEW.document_id;

    -- Insertar nueva versión
    INSERT INTO document_versions (
      version_id,
      document_id,
      content,
      created_by,
      created_at,
      version_number,
      audit_data
    ) VALUES (
      gen_random_uuid(),
      NEW.document_id,
      OLD.content,
      OLD.created_by,
      now(),
      next_version,
      jsonb_build_object(
        'source', 'auto-trigger',
        'timestamp', now()
      )
    );
  END IF;
  RETURN NEW;
END;
$$;


